{
  "name": "ixfx",
  "version": "0.9.0",
  "description": "A framework for programming interactivity",
  "homepage": "https://github.com/clinth/ixfx/",
  "keywords": [
    "interactivity"
  ],
  "author": "Clint Heyer",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:clinth/ixfx.git"
  },
  "module": "./dist/bundle.js",
  "exports": {
    ".": "./dist/bundle.js",
    "./lib/*": "./dist/*.js",
    "./collections.js": "./dist/collections.js",
    "./arrays.js": "./dist/arrays.js",
    "./components.js": "./dist/components.js",
    "./data.js": "./dist/data.js",
    "./dom.js": "./dist/dom.js",
    "./events.js": "./dist/events.js",
    "./forms.js": "./dist/forms.js",
    "./flow.js": "./dist/flow.js",
    "./geometry.js": "./dist/geometry.js",
    "./generators.js": "./dist/generators.js",
    "./io.js": "./dist/io/js",
    "./keyValues.js": "./dist/keyValues.js",
    "./modulation.js": "./dist/modulation.js",
    "./maps.js": "./dist/maps.js",
    "./numbers.js": "./dist/numbers.js",
    "./queues.js": "./dist/queues.js",
    "./random.js": "./dist/random.js",
    "./sets.js": "./dist/sets.js",
    "./stateMachine.js": "./dist/stateMachine.js",
    "./svg.js": "./dist/svg.js",
    "./text.js": "./dist/text.js",
    "./util.js": "./dist/util.js",
    "./visual.js": "./dist/visual.js"
  },
  "browsersList": [
    ">2% and last 2 versions and not ie 6-11"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc",
    "develop_": "tsc --watch",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf etc && rimraf docs",
    "run": "tsc && node -r source-map-support/register",
    "build": "tsup --sourcemap && npm run typings",
    "build:espruino": "tsup --config espruino.tsup.config.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "typings": "tsc --project config/tsconfig.pkg.json",
    "typings:watch": "tsc --watch --project config/tsconfig.pkg.json",
    "demos": "rimraf dist && npm run build && npm run demos:copy",
    "demos:watch": "concurrently \"npm run dev\"  \"npm run typings:watch\"",
    "demos:copy": "copyfiles --up 1 ./dist/* ../ixfx-demos/ixfx/",
    "docs": "typedoc --customCss ./config/custom.css --options ./config/typedoc.json",
    "docs:watch": "typedoc --customCss ./config/custom.css --watch --options ./config/typedoc.json",
    "docs:copyIcons": "copyfiles --up 1 ./config/*.png ./docs/assets/",
    "lint": "eslint src/**",
    "test:coverage": "jest --collectCoverage --config config/jest.config.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config config/jest.config.js",
    "tsc-files": "tsc --noEmit --listFiles | xargs stat -f \"%z %N\" | npx webtreemap-cli",
    "ava": "tsc && ava --config ./config/ava.config.js"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@svgdotjs/svg.js": "^3.2.0",
    "@types/d3-color": "^3.1.0",
    "@types/d3-interpolate": "^3.0.1",
    "@types/jest": "^29.5.2",
    "@types/w3c-web-serial": "^1.0.3",
    "@types/web-bluetooth": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.1",
    "ava": "^5.3.1",
    "concurrently": "^8.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "d3-interpolate": "^3.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "fp-ts": "^2.16.0",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "live-server": "^1.2.2",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "tsup": "^7.1.0",
    "tsutils": "^3.21.0",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/bezier-js": "^4.1.0",
    "bezier-js": "^6.1.3",
    "d3-color": "^3.1.0",
    "json5": "^2.2.3",
    "lit": "^2.7.5",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=17.0.1"
  },
  "eslintIgnore": [
    "./dist/**/*"
  ],
  "ava": {
    "files": [
      "./src/test/*.ts"
    ],
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
