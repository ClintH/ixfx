{
  "name": "ixfx",
  "version": "0.20.0",
  "description": "A framework for programming interactivity",
  "homepage": "https://github.com/clinth/ixfx/",
  "keywords": [
    "interactivity"
  ],
  "author": "Clint Heyer",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:clinth/ixfx.git"
  },
  "module": "./dist/bundle.js",
  "exports": {
    ".": "./dist/bundle.js",
    "./lib/*": "./dist/*.js",
    "./collections.js": "./dist/collections.js",
    "./arrays.js": "./dist/arrays.js",
    "./components.js": "./dist/components.js",
    "./webcomponents.js": "./dist/webcomponents.js",
    "./data.js": "./dist/data.js",
    "./debug.js": "./dist/debug.js",
    "./dom.js": "./dist/dom.js",
    "./events.js": "./dist/events.js",
    "./forms.js": "./dist/forms.js",
    "./flow.js": "./dist/flow.js",
    "./geometry.js": "./dist/geometry.js",
    "./guards.js": "./dist/guards.js",
    "./global.js": "./dist/global.js",
    "./immutable.js": "./dist/immutable.js",
    "./iterables.js":"./dist/iterable.js",
    "./io.js": "./dist/io/js",
    "./keyValues.js": "./dist/keyValues.js",
    "./modulation.js": "./dist/modulation.js",
    "./maps.js": "./dist/maps.js",
    "./numbers.js": "./dist/numbers.js",
    "./queues.js": "./dist/queues.js",
    "./random.js": "./dist/random.js",
    "./rx.js": "./dist/rx.js",
    "./sets.js": "./dist/sets.js",
    "./stateMachine.js": "./dist/stateMachine.js",
    "./svg.js": "./dist/svg.js",
    "./text.js": "./dist/text.js",
    "./trees.js": "./dist/trees.js",
    "./util.js": "./dist/util.js",
    "./visual.js": "./dist/visual.js"
  },
  "browsersList": [
    ">2% and last 2 versions and not ie 6-11"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc",
    "watch": "tsc --watch",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf etc && rimraf docs && rimraf .tsimp",
    "run": "tsc && node -r source-map-support/register",
    "build": "tsup --sourcemap && npm run typings",
    "build:espruino": "tsup --config espruino.tsup.config.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "typings": "tsc --project config/tsconfig.pkg.json",
    "typings:watch": "tsc --watch --project config/tsconfig.pkg.json",
    "demos": "rimraf ../ixfx-demos/ixfx/ && rimraf dist && npm run build && npm run demos:copy",
    "demos:watch": "concurrently \"npm run dev\"  \"npm run typings:watch\"",
    "demos:copy": "copyfiles --up 1 ./dist/* ../ixfx-demos/ixfx/",
    "docs": "typedoc --customCss ./config/custom.css --options ./config/typedoc.json",
    "docs:watch": "typedoc --customCss ./config/custom.css --watch --options ./config/typedoc.json",
    "docs:copyIcons": "copyfiles --up 1 ./config/*.png ./docs/assets/",
    "lint": "eslint src/**",
    "tsc-files": "tsc --noEmit --listFiles | xargs stat -f \"%z %N\" | npx webtreemap-cli",
    "test": "tsc && ava --config ./config/ava.config.js --timeout=20s --no-worker-threads -c=1",
    "circular": "madge --circular --extensions ts src/ > madge.txt",
    "coverage": "c8 npm run test",
    "coverage:report": "c8 report"
  },
  "c8": {
    "exclude": ["src/__test/**/*", "src/config/**/*"]
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@svgdotjs/svg.js": "^3.2.0",
    "@types/d3-color": "^3.1.3",
    "@types/d3-interpolate": "^3.0.4",
    "@types/jsdom": "^21.1.6",
    "@types/w3c-web-serial": "^1.0.6",
    "@types/web-bluetooth": "^0.0.20",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "ava": "^6.1.2",
    "c8": "^9.1.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "d3-interpolate": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^6.3.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jsdom": "^24.0.0",
    "live-server": "^1.2.2",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "tsup": "^8.0.2",
    "tsutils": "^3.21.0",
    "tsx": "^4.7.1",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@septh/ts-run": "^1.2.2",
    "@types/bezier-js": "^4.1.3",
    "bezier-js": "^6.1.4",
    "d3-color": "^3.1.0",
    "json5": "^2.2.3",
    "lit": "^3.1.1",
    "lit-html": "^3.1.2",
    "tsimp": "^2.0.11"
  },
  "eslintIgnore": [
    "./dist/**/*"
  ]
}
