{
  "name": "@clinth/ix",
  "version": "0.0.1",
  "description": "",
  "module": "dist/bundle.min.mjs",
  "unpkg": "dist/bundle.umd.min.js",
  "exports": "./dist/bundle.mjs",
  "browsersList": [
    ">2% and last 2 versions and not ie 6-11"
  ],
  "main": "dist/bundle.js",
  "type": "module",
  "types": "dist/bundle.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:coverage": "jest --collectCoverage",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "prepublish": "npm run build",
    "build": "tsc && npm run bundle",
    "run": "tsc && node -r source-map-support/register",
    "esbundle": "esbuild src/index.ts --sourcemap --format=esm --outfile=dist/bundle.mjs --bundle",
    "esdevelop": "esbuild src/index.ts --sourcemap --format=esm --outfile=dist/bundle.mjs --bundle --watch",
    "bundle": "tsup",
    "develop": "tsup --watch",
    "dev:demos": "snowpack dev --config ./snowpack-demos.config.js",
    "dev:demosAlt": "snowpack dev --config ./snowpack-demosAlt.config.js",
    "build:demos": "snowpack build --config ./snowpack-demos.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@svgdotjs/svg.js": "^3.1.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.1",
    "color2k": "^1.2.5",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.11",
    "eslint": "^8.6.0",
    "eslint-plugin-functional": "^4.1.1",
    "fp-ts": "^2.11.7",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "live-server": "^1.2.1",
    "npm-check-updates": "^12.1.0",
    "rimraf": "^3.0.2",
    "snowpack": "^3.8.8",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.11",
    "tsutils": "^3.21.0",
    "typescript": "^4.6.0-dev.20220109"
  },
  "repository": {
    "url": "git@github.com:clinth/ix.git"
  },
  "dependencies": {
    "@types/bezier-js": "^4.1.0",
    "bezier-js": "^4.1.1",
    "lit": "^2.1.1",
    "rxjs": "^7.5.2"
  }
}
