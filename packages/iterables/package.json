{
	"name": "@ixfx/iterables",
	"version": "0.45.0",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist/",
		"bundle/"
	],
	"scripts": {
		"package-bundle": "tsdown",
		"clean": "rm -rf dist && rm -rf bundle"
	},
	"dependencies": {
		"@ixfx/arrays": "workspace:^",
		"@ixfx/guards": "workspace:^",
		"@ixfx/numbers": "workspace:^",
		"@ixfx/process": "workspace:^",
		"@ixfx/core": "workspace:^"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./sync": {
			"types": "./src/sync.ts",
			"import": "./src/sync.ts"
		},
		"./async": {
			"types": "./src/async.ts",
			"import": "./src/async.ts"
		}
	},
	"publishConfig": {
		"main": "dist/src/index.js",
		"exports": {
			".": {
				"types": "./dist/src/index.d.ts",
				"import": "./dist/src/index.js"
			},
			"./sync": {
				"types": "./dist/src/sync.d.ts",
				"import": "./dist/src/sync.js"
			},
			"./async": {
				"types": "./dist/src/async.d.ts",
				"import": "./dist/src/async.js"
			}
		}
	}
}