{
	"name": "@ixfx/ixfx",
	"version": "0.38.0",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"homepage": "https://clinth.github.io/ixfx/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/clinth/ixfx.git"
	},
	"files": [
		"dist/*"
	],
	"exports": {
		".": {
			"types": "./index.ts",
			"import": "./index.ts"
		},
		"./numbers": {
			"types": "./numbers.ts",
			"import": "./numbers.ts"
		},
		"./visual": {
			"types": "./visual.ts",
			"import": "./visual.ts"
		},
		"./rx": {
			"types": "./rx/index.ts",
			"import": "./rx/index.ts"
		}
	},
	"dependencies": {
		"@ixfx/arrays": "workspace:^",
		"@ixfx/collections": "workspace:^",
		"@ixfx/core": "workspace:^",
		"@ixfx/debug": "workspace:^",
		"@ixfx/dom": "workspace:^",
		"@ixfx/events": "workspace:^",
		"@ixfx/flow": "workspace:^",
		"@ixfx/geometry": "workspace:^",
		"@ixfx/guards": "workspace:^",
		"@ixfx/iterables": "workspace:^",
		"@ixfx/modulation": "workspace:^",
		"@ixfx/numbers": "workspace:^",
		"@ixfx/random": "workspace:^",
		"@ixfx/rx": "workspace:^",
		"@ixfx/visual": "workspace:^"
	},
	"publishConfig": {
		"main": "dist/index.js",
		"types": "dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js"
			},
			"./rx": {
				"types": "./dist/rx/index.d.ts",
				"import": "./dist/rx/index.js"
			},
			"./numbers": {
				"types": "./dist/numbers.d.ts",
				"import": "./dist/numbers.js"
			},
			"./visual": {
				"types": "./dist/visual.d.ts",
				"import": "./dist/visual.js"
			}
		}
	},
	"devDependencies": {
		"esbuild": "0.25.2"
	}
}
