{
	"name": "@ixfx/ui",
	"version": "0.47.0",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist/",
		"bundle/"
	],
	"scripts": {
		"package-bundle": "tsdown",
		"clean": "rm -rf dist && rm -rf bundle"
	},
	"dependencies": {
		"@ixfx/core": "workspace:^",
		"@ixfx/collections": "workspace:^",
		"@ixfx/dom": "workspace:^",
		"@ixfx/geometry": "workspace:^",
		"@ixfx/numbers": "workspace:^",
		"@ixfx/visual": "workspace:^",
		"@ixfx/rx": "workspace:^"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		}
	},
	"publishConfig": {
		"main": "dist/src/index.js",
		"exports": {
			".": {
				"types": "./dist/src/index.d.ts",
				"import": "./dist/src/index.js"
			}
		}
	}
}